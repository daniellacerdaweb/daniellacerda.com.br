{
  "name": "with-typescript",
  "version": "1.0.0",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc",
    "format": "prettier-eslint --write $PWD/'**/*.{ts,tsx}'",
    "lint": "eslint --ext .ts,.tsx .",
    "download-schema": "node script/download-schema.js",
    "codegen": "npm run download-schema && apollo codegen:generate --localSchemaFile=graphql-schema.json,client-schema.graphql --target=typescript --tagName=gql"
  },
  "dependencies": {
    "@apollo/client": "^3.3.16",
    "@contentful/rich-text-react-renderer": "^14.1.3",
    "@contentful/rich-text-types": "^14.1.2",
    "contentful": "^8.3.2",
    "dotenv": "^9.0.0",
    "framer-motion": "^4.1.11",
    "graphql": "^15.5.0",
    "next": "latest",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-lazy-load-image-component": "^1.5.1",
    "react-scroll": "^1.8.2"
  },
  "devDependencies": {
    "@types/node": "^12.12.21",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/react-lazy-load-image-component": "^1.5.1",
    "@types/react-scroll": "^1.8.2",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "autoprefixer": "^10.2.5",
    "eslint": "^7.25.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "postcss": "^8.2.13",
    "prettier": "^2.2.1",
    "prettier-eslint-cli": "^5.0.1",
    "stylelint-config-recommended": "^5.0.0",
    "tailwindcss": "^2.1.2",
    "typescript": "4.0"
  },
  "license": "MIT"
}
